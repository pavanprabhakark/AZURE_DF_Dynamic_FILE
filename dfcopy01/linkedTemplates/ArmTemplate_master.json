{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfcopy01"
		},
		"LNK_BLB_TO BLB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LNK_BLB_TO BLB'"
		},
		"LNK_BLOBTOBLOB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LNK_BLOBTOBLOB'"
		},
		"LN_srccon1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LN_srccon1'"
		},
		"LN_trgcon1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LN_trgcon1'"
		},
		"LS_Datawithlatestdate_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Datawithlatestdate'"
		},
		"LS_TRG_DataWithlatestdate_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_TRG_DataWithlatestdate'"
		},
		"LS_TRG_DataWithlatestdate_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://destsa03.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfcopy01_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LNK_BLB_TO BLB_connectionString": {
						"value": "[parameters('LNK_BLB_TO BLB_connectionString')]"
					},
					"LNK_BLOBTOBLOB_connectionString": {
						"value": "[parameters('LNK_BLOBTOBLOB_connectionString')]"
					},
					"LN_srccon1_connectionString": {
						"value": "[parameters('LN_srccon1_connectionString')]"
					},
					"LN_trgcon1_connectionString": {
						"value": "[parameters('LN_trgcon1_connectionString')]"
					},
					"LS_Datawithlatestdate_connectionString": {
						"value": "[parameters('LS_Datawithlatestdate_connectionString')]"
					},
					"LS_TRG_DataWithlatestdate_accountKey": {
						"value": "[parameters('LS_TRG_DataWithlatestdate_accountKey')]"
					},
					"LS_TRG_DataWithlatestdate_properties_typeProperties_url": {
						"value": "[parameters('LS_TRG_DataWithlatestdate_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}